@{
    ViewData["Title"] = "Archipelago End Game";
}

<style>
h3, h4, ol, ul, p {
    margin-top: 0;
    margin-bottom: 0;
}

h3 {
    margin-bottom: .5em;
}

ol, ul {
    margin-bottom: 1em;
}

li > ul {
    margin-bottom: 0;
}

footer {
    margin-bottom: 0;
}

.icon {
    width: 50px;
}

.count {
    /*border: 1px solid black;*/
    position: relative;
    width: 45px;
    height: 45px;
    background-image: url('@Url.Content("~/lib/Images/playercount.png")');
    background-size: 100%;
    background-position-x: 0;
    background-position-y: 0;
    background-repeat: no-repeat;
    -webkit-print-color-adjust: exact;
    margin-right: 2em;
}

.count h4 {
    position: absolute;
    left: 43%;
    top: 40%;
}

.titlewrap {
    margin-left: 1em;
}

.titlewrap > * {
    display: inline-block;
    vertical-align: middle;
}

.archipelagotable .table > .table-row {
    padding-top: .5em;
    padding-bottom: .5em;
    padding-left: 1em;
    padding-right: 1em;
}
</style>

